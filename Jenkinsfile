    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..1'
            }
          steps {
                echo 'Building..2'
            }
          steps {
                echo 'Building..3'
            }
         
        }
        stage('Test') {
            steps {
                echo 'Testing..1'
            }
          steps {
                echo 'Testing..2'
            }
          steps {
                echo 'Testing..3'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....1'
            }
          steps {
                echo 'Deploying....2'
            }
          steps {
                echo 'Deploying....3'
            }
        }
    }
}
